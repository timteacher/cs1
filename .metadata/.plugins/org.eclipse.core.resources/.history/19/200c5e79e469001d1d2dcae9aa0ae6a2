package chapter8;/

public class Employee {
	private String name;
	private double baseSalary;
	public void setName(String newName) {
		name = newName;
	}
	public void setBaseSalary(double newSalary) {
		baseSalary = newSalary;
	}
	public String getName() {
		return name;
	}
	public double getSalary() {
		return baseSalary;
	}

}
